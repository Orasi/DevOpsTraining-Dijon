%tr
  %td.table-number
    = "#{result_counter + 1}."
  %td
    = link_to result.project_name, execution_path(result.execution_id)
  %td
    = result.testcase_name
  %td
    - if result['environment_display_name']
      = result['environment_display_name']
    - elsif result['environment_id'] == -1
      = 'Manual'
    - else
      = result.result['environment_id']
  %td
    = build_status result.result['status']
  %td
    .small.overview-time.pull-right
      Updated:
      = time_ago_in_words( result.updated_at) + ' ago'
