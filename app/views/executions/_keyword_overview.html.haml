
.row
  .col-md-12
    %table.table.table-condensed.table-overview.numbered_table
      %tbody
        - keywords.each_with_index do |keyword, i|
          %tr
            - results_bar = stacked_progress_bar :Results, keyword['expected_count'], keyword['pass_count'], keyword['fail_count'], keyword['skip_count']
            %td
              .row-index
            %td
              = keyword['keyword'] ? keyword['keyword'] : 'No Keyword'
            %td
              .stacked_progress
                = results_bar
            %td
              .pull-right.small.overview-time
                -if keyword['updated_at']
                  .timeago{title: DateTime.parse(keyword['updated_at']).iso8601}
            %td
              .pull-right

  :javascript
    $('.timeago').timeago()